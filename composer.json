{
    "name": "rawilk/laravel-casters",
    "description": "A collection of custom casts for Laravel.",
    "keywords": [
        "rawilk",
        "laravel-casters",
        "custom casts",
        "password cast",
        "encryption cast"
    ],
    "homepage": "https://github.com/rawilk/laravel-casters",
    "license": "MIT",
    "authors": [
        {
            "name": "Randall Wilk",
            "email": "randall@randallwilk.dev",
            "homepage": "https://randallwilk.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "illuminate/contracts": "^9.0",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.3",
        "pestphp/pest-plugin-parallel": "^1.2",
        "spatie/laravel-ray": "^1.31"
    },
    "autoload": {
        "psr-4": {
            "Rawilk\\LaravelCasters\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rawilk\\LaravelCasters\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest -p",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
